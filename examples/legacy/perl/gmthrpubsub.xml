<DEFINITIONS>
	<CONFIG NAME="config_bolt">
		<PARAMETER NAME="connectiontype">gmsec_bolt</PARAMETER>
		<PARAMETER NAME="server">localhost</PARAMETER>
	</CONFIG>

	<CONFIG NAME="config_mb">
		<PARAMETER NAME="connectiontype">gmsec_mb</PARAMETER>
		<PARAMETER NAME="server">localhost</PARAMETER>
	</CONFIG>

	<CONFIG NAME="config_ss66">
		<PARAMETER NAME="connectiontype">gmsec_ss66</PARAMETER>
		<PARAMETER NAME="server">ei-ws1</PARAMETER>
		<PARAMETER NAME="version">true</PARAMETER>
		<PARAMETER NAME="Server_Keep_Alive_Timeout">5</PARAMETER>
		<PARAMETER NAME="CommandString">setopt server_read_timeout 5; setopt server_write_timeout 5; setopt _socket_linger FALSE; setopt _socket_linger_time 0.0; setopt server_max_reconnect_delay 0; setopt socket_connect_timeout 3.0</PARAMETER>
	</CONFIG>
</DEFINITIONS>
