<?xml version="1.0" encoding="UTF-8"?>
<DEFINITIONS>

    <SUBSCRIPTION NAME="all-messages" PATTERN="GMSEC.>">
        <EXCLUDE PATTERN="GMSEC.*.*.MSG.HB.>"/>
        <EXCLUDE PATTERN="GMSEC.*.*.MSG.LOG.*"/>
    </SUBSCRIPTION>

    <SUBSCRIPTION NAME="custom1" PATTERN="CUSTOM.MESSAGE.SUBJECTS.*"/>

    <CONFIG NAME="config1">
        <PARAMETER NAME="mw-id">activemq395</PARAMETER>
        <PARAMETER NAME="mw-server">tcp://myserver:61616</PARAMETER>
    </CONFIG>

    <CONFIG NAME="config2">
        <PARAMETER NAME="mw-id">bolt</PARAMETER>
        <PARAMETER NAME="mw-server">10.1.2.159</PARAMETER>
        <PARAMETER NAME="tracking">false</PARAMETER>
    </CONFIG>

    <MESSAGE NAME="msg1" SUBJECT="GMSEC.MISSION.SAT.MSG.HB.MY-COMPONENT" KIND="PUBLISH">
        <CONFIG NAME="msg_config">
            <PARAMETER NAME="GMSEC-MSGFLD-STORE-SIZE">15</PARAMETER>
            <PARAMETER NAME="GMSEC-MSGFLD-STORE-TYPE">tree</PARAMETER>
            <PARAMETER NAME="GMSEC-IDENTIFY-HEADER-FIELDS">true</PARAMETER>
        </CONFIG>

        <FIELD NAME="COMPONENT" TYPE="STRING" HEAD="T">MY-COMPONENT</FIELD>
        <FIELD NAME="CONSTELLATION-ID" TYPE="STRING" HEAD="T">CNS1</FIELD>
        <FIELD NAME="CONTENT-VERSION" TYPE="F32" BITS="44FC6000">2019</FIELD>
        <FIELD NAME="COUNTER" TYPE="U16">1</FIELD>
        <FIELD NAME="DOMAIN1" TYPE="STRING" HEAD="T">DOM1</FIELD>
        <FIELD NAME="DOMAIN2" TYPE="STRING" HEAD="T">DOM2</FIELD>
        <FIELD NAME="FACILITY" TYPE="STRING" HEAD="T">MY-FACILITY</FIELD>
        <FIELD NAME="HEADER-VERSION" TYPE="F32" BITS="44FC6000" HEAD="T">2019</FIELD>
        <FIELD NAME="MESSAGE-SUBTYPE" TYPE="STRING" HEAD="T">HB</FIELD>
        <FIELD NAME="MESSAGE-TYPE" TYPE="STRING" HEAD="T">MSG</FIELD>
        <FIELD NAME="MISSION-ID" TYPE="STRING" HEAD="T">MISSION</FIELD>
        <FIELD NAME="PUB-RATE" TYPE="U16">30</FIELD>
        <FIELD NAME="SAT-ID-LOGICAL" TYPE="STRING" HEAD="T">SAT</FIELD>
        <FIELD NAME="SAT-ID-PHYSICAL" TYPE="STRING" HEAD="T">SAT</FIELD>
        <FIELD NAME="SPECIFICATION" TYPE="STRING" HEAD="T">GMSEC</FIELD>
        <FIELD NAME="SW-VERSION" TYPE="STRING">5.1</FIELD>
    </MESSAGE>

    <MESSAGE NAME="msg2" SUBJECT="GMSEC.MISSION.SAT.MSG.LOG.MY-COMPONENT" KIND="PUBLISH">
        <FIELD NAME="COMPONENT" TYPE="STRING" HEAD="T">MY-COMPONENT</FIELD>
        <FIELD NAME="CONSTELLATION-ID" TYPE="STRING" HEAD="T">CNS1</FIELD>
        <FIELD NAME="CONTENT-VERSION" TYPE="F32" BITS="44FC6000">2019</FIELD>
        <FIELD NAME="DOMAIN1" TYPE="STRING" HEAD="T">DOM1</FIELD>
        <FIELD NAME="DOMAIN2" TYPE="STRING" HEAD="T">DOM2</FIELD>
